{
  "Application": {
    "LocalDbConnectionString": "Data Source=localhost\\sqlexpress;Initial Catalog=MaiBookmarks;Integrated Security=True;Encrypt=False;"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "{NewLine}{Timestamp:HH:mm:ss} [{Level}] ({CorrelationToken}) {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Debug",
        "Args": {
          "outputTemplate": "{NewLine}{Timestamp:HH:mm:ss} [{Level}] ({CorrelationToken}) {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Protein.HeadCondemn-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{NewLine}{Timestamp:HH:mm:ss} [{Level}] ({CorrelationToken}) {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Udp",
        "Args": {
          "remoteAddress": "localhost",
          "remotePort": 44445,
          "family": "InterNetwork",
          "outputTemplate": "{NewLine}{Timestamp:HH:mm:ss} [{Level}] ({CorrelationToken}) {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=localhost\\sqlexpress;Initial Catalog=HeadCondemn;Integrated Security=True;;Encrypt=False;",
          "sinkOptionsSection": {
            "tableName": "Log"
          },
          "columnOptionsSection": {
            "removeStandardColumns": [ "Id", "LogEvent", "MessageTemplate", "Properties" ],
            "additionalColumns": [
              {
                "ColumnName": "Logger",
                "DataType": "varchar",
                "DataLength": "100"
              }
            ]
          }
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithExceptionDetails"
    ]
  }
}